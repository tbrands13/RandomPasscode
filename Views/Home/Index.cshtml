<h1>Random Passcode Generator</h1>
<form asp-controller="Home" asp-action="Submit" method="POST">
    <h4>Random passcode (passcode #)</h4>
    <input type="text" class="form-control" name="passcode" value="@ViewBag.Password"/>
    <input type="submit" value="Generate" class=" btn btn-secondary btn-lg"/>
</form>
